@import "~common/stylus/variable.styl"
.m-header
  width 100%
  height 60px
  line-height 60px
  background $header-background
  border-bottom 1px solid $header-bottom-color
  .icon
    display inline-block
    vertical-align top
    width 50px
    height 42px
    background url('~common/image/logo.svg')
    margin-left 21px
    margin-top 18px
  .title
    vertical-align top
    display inline-block
    font-size 20px