.projectsetting
  display flex
  color white
  font-size 14px
  color #4f5d83
  .split
    height 10px
    line-height 10px
  .moveflag
    position absolute
    width 20px
    height 20px
    z-index 100001
    border 1px dashed fuchsia

  .rightWrapper
    flex 1
    .editor
      position: absolute;
      -webkit-box-sizing: border-box;
      box-sizing: border-box;
      bottom 0
      left: 290px
      top: 60px
      right 300px
      .editor-panel
        position: absolute;
        top: 0px;
        left: 0px;
        width: 100%;
        height: 100%;
        box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 5px 0px;
        background: rgb(255, 255, 255);
        overflow: hidden;
        font-size 0
        .el-menu-demo
          background-image url(/img/background/dragon.jpg)
          filter opacity(80%)
          .previewText
            display inline-block
        .editor-body
          position absolute
          left 0
          top 60px
          right 0px
          z-index 3
          bottom 0
          padding-bottom 150px
          background-color #f0f3f4
          overflow auto
          .containerWrapper
            position relative
            //background transparent
            //background-color  #808080
            //background linear-gradient(-90deg, rgba(0, 0, 0, .1) 1px, transparent 1px), linear-gradient(rgba(0, 0, 0, .1) 1px, transparent 1px)
            //background-size  20px 20px, 20px 20px
            .dashboard-background-image
              position: relative;
              width: 1920px;
              height: 1080px;
              background:none
              &.active
                background: rgb(0, 0, 0);
                transform: scale(1.0, 1.0);
                transform-origin: 0 0;
                user-select: none;
                box-shadow: 0 0 10px 0 rgba(0,0,0,.5);
                transition: all .3s linear;
              //background url(/img/background/background-4.png) 0% 0% / 100% 100%
              //filter blur(0px)
              //opacity 0.7
         .bottom-panel
           position absolute
           bottom 0
           right 0
           z-index 3
           height 2em
           line-height 2em
           font-size 18px
           .range
             width 200px
           .number
             width 3em
             line-height: 1.15
             margin-left: .5em
           .el-icon-full-screen
             margin-left: .5em
      .loading
        position absolute
        display flex
        align-items center
        justify-content center
        width 1150px
        height 700px
        z-index 20
        .loading-container
          background-color rgb(26, 188, 156)
          padding 20px 40px
          border-radius 5px
          .loading-title
            display flex
            align-items center
            justify-content center
            height 30px
            line-height 30px
            color white
    .css-fullconfig
      background-image url(/img/background/dragon.jpg)
      filter opacity(80%)
      position absolute
      width 300px
      top 60px
      bottom 0
      right 0
      .configheader
        line-height 60px
        padding-left 10px
      .nocontent
        position absolute
        top 50%
        left 50%
        transform: translate(-50%, -50%)
        color orange
      .content
        position absolute
        top 50px
        bottom 0
        right 0
        left 0
        overflow auto
        .tag
          margin-bottom 10px
        .setting
          margin-bottom 10px
  .sqltablefieldsdelete
    text-align center
  .fieldsWrapper::-webkit-scrollbar,.editor-body::-webkit-scrollbar,.css-fullconfig .content::-webkit-scrollbar,.leftWrapper::-webkit-scrollbar
    width 5px
    height 5px
  .fieldsWrapper::-webkit-scrollbar-thumb,.editor-body::-webkit-scrollbar-thumb,.css-fullconfig .content::-webkit-scrollbar-thumb,.leftWrapper::-webkit-scrollbar-thumb
    background-color rgba(150,150,150,0.8)
  .fieldsWrapper::-webkit-scrollbar-track,.editor-body::-webkit-scrollbar-track,.css-fullconfig .content::-webkit-scrollbar-track,.leftWrapper::-webkit-scrollbar-track
    background-color rgba(255,255,255,0.3)
  .el-tabs__item:hover
    color #ffa500
  .el-tabs__item.is-active
    color #ffa500
  .el-tree--highlight-current .el-tree-node.is-current>.el-tree-node__content
      background-color #ffa500


